const {register, verifyTeacher, resendVerification} = require('../UserControllers/TeacherController')



const router = require('express').Router()



try {
  router.post("/register", register);
  router.get("/verify/:token", verifyTeacher)
  router.get("/resendVerification/:token", resendVerification)
  
} catch (err) {
  console.error("Route definition error:", err);
  process.exit(1);
}



module.exports = router